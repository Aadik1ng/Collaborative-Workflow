{
  "devCommand": "uvicorn app.main:app --reload",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "app/main.py"
    },
    {
      "source": "/docs",
      "destination": "app/main.py"
    },
    {
      "source": "/openapi.json",
      "destination": "app/main.py"
    }
  ]
}
